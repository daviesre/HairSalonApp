<h1>Stylist List</h1>
<hr/>
<h2>Are you sure you want to delete this stylist and all of their clients?</h2>
<h4>@Model.GetName()</h4>
<form action="/stylist/delete/@Model.GetId()" method="post">
  <input type="hidden" name="_method" value="DELETE">
  <button type="submit">Delete</button>
</form>
<p><a href="/">Return home</a></p>
